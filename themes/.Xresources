!#####
!# X #
!#####

! X config
Xcursor.size: 2

!##########
!# COLORS #
!##########

!! black
*color0:  #040728
*color8:  #000232

!! red
*color1:  #bf052d
*color9:  #e60434

!! green
*color2 : #549776
*color10: #6bd5ff

!! yellow
*color3:  #82a569
*color11: #b3ffbb

!! blue
*color4:  #124ab0
*color12: #1f6bd5

!! magenta
*color5:  #c2518a
*color13: #e86f95

!! cyan
*color6:  #31a6e4
*color14: #76cadb

!! white
*color7:  #dfe4d7
*color15: #fdfefe

!#########
!# URXVT #
!#########

! scrollbar
URxvt.scrollBar:	false
URxvt.scrollBar_right:	false
URxvt.scrollstyle: 	plain
URxvt.thickness:	0

! padding
URxvt.internalBorder:	25

URxvt.depth:            30
URxvt.saveLines:        10000

URxvt.foreground:       #fdfefe
URxvt.background:       #040728

!#########
!# FONTS #
!#########

! fonts
Xft.autohint:		1
Xft.antialias:          1
Xft.hinting:            true
Xft.hintstyle:          hintfull
Xft.lcdfilter:          lcddefault
Xft.rgba:               rgb

*URxvt.font:		xft:Victor Mono:size=10
URxvt.boldFont:         xft:Victor Mono Bold:size=10
!URxvt.lineSpace:	10
!URxvt.letterSpace:	0
