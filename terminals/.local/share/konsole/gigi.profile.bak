TextEditorCmdCustom=emacsclient -a -c PATH:LINE:COLUMN
TrimLeadingSpacesInSelectedText=false
TrimTrailingSpacesInSelectedText=false
TripleClickMode=0
UnderlineFilesEnabled=false
UnderlineLinksEnabled=true
WordCharacters=:@-./_~?&=%+#

[Keyboard]
KeyBindings=default

[Scrolling]
HighlightScrolledLines=true
HistoryMode=2
HistorySize=1000
ReflowLines=true
ScrollBarPosition=2
ScrollFullPage=false

[Terminal Features]
