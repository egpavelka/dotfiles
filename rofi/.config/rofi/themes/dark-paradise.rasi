/**
 * rofi -dump-theme output.
 * Rofi version: 1.5.0-dev
 **/
* {
    selected-normal-foreground:  #fdfefeFF;
    foreground:                  #dfe4d7FF;
    normal-foreground:           @foreground;
    alternate-normal-background: #0407282f;
    red:                         #bf052d79;
    selected-urgent-foreground:  @selected-urgent-foreground;
    blue:                        #549776FF;
    urgent-foreground:           @red;
    alternate-urgent-background: @lightbg;
    active-foreground:           @blue;
    lightbg:                     #e60434FF;
    selected-active-foreground:  @background;
    alternate-active-background: @lightbg;
    background:                  #040728AA;
    alternate-normal-foreground: @foreground;
    normal-background:           #000232AA;
    lightfg:                     #1f6bd5FF;
    selected-normal-background:  #76cadb9B;
    border-color:                #b3ffbbFF;
    spacing:                     2;
    separatorcolor:              #b3ffbbFF;
    urgent-background:           @background;
    selected-urgent-background:  @red;
    alternate-urgent-foreground: @red;
    background-color:            #04072800;
    alternate-active-foreground: @blue;
    active-background:           @background;
    selected-active-background:  @blue;
}
window {
    background-color: @background;
    border:           0;
    transparency:     "background";
    padding:          25%;
}
mainbox {
    border:  2px 0 0;
    border-color: @separatorcolor;
    padding: 0;
}
message {
    border:       2px 0px 0px ;
    border-color: @separatorcolor;
    padding:      10px ;
}
textbox {
    text-color: @foreground;
}
listview {
    fixed-height: 0;
    border:       2px 0px 0px ;
    border-color: @separatorcolor;
    spacing:      2px ;
    scrollbar:    false;
    padding:      2px 0px 0px ;
}
element {
    border:  0;
    padding: 10px ;
}
element normal.normal {
    background-color: @normal-background;
    text-color:       @normal-foreground;
}
element normal.urgent {
    background-color: @urgent-background;
    text-color:       @urgent-foreground;
}
element normal.active {
    background-color: @active-background;
    text-color:       @active-foreground;
}
element selected.normal {
    background-color: @selected-normal-background;
    text-color:       @selected-normal-foreground;
}
element selected.urgent {
    background-color: @selected-urgent-background;
    text-color:       @selected-urgent-foreground;
}
element selected.active {
    background-color: @selected-active-background;
    text-color:       @selected-active-foreground;
}
element alternate.normal {
    background-color: @alternate-normal-background;
    text-color:       @alternate-normal-foreground;
}
element alternate.urgent {
    background-color: @alternate-urgent-background;
    text-color:       @alternate-urgent-foreground;
}
element alternate.active {
    background-color: @alternate-active-background;
    text-color:       @alternate-active-foreground;
}
scrollbar {
    width:        4px ;
    border:       0;
    handle-color: @normal-foreground;
    handle-width: 8px ;
    padding:      0;
}
sidebar {
    border:       2px 0px 0px ;
    border-color: @separatorcolor;
}
button {
    spacing:    0;
    font:       "EtBembo 20";
    margin:     10px 0;
    padding:    5px 0 15px;
    text-color: @normal-foreground;
}
button selected {
    background-color: @selected-normal-background;
    text-color:       @selected-normal-foreground;
}
inputbar {
    text-color: @normal-foreground;
    padding:    10px;
    children:   [ prompt,entry ];

}
case-indicator {
    text-color: @normal-foreground;
}
entry {
    padding:    25px 0 0;
    text-color: @normal-foreground;
}

prompt {
    padding:    0 10px 40px;
    text-color: @separatorcolor;
    font:       "Symbola 36";
}

textbox-prompt-colon {
    expand:     false;
    str:        "---";
    margin:     0px 0.3000em 0.0000em 0.0000em ;
    text-color: inherit;
}
